   "source": [
    "# Write a Python program to find the values of length six in a given list using Lambda\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Banana\n",
      "Grapes\n",
      "Orange\n"
     ]
    }
   ],
   "source": [
    "given_list = [\"Apple\",\"Banana\",\"Grapes\",\"Guava\",\"Mango\",\"Orange\"]\n",
    "\n",
    "fruits= filter(lambda fruit : fruit if len(fruit)==6 else \"\",given_list)\n",
    "\n",
    "for f in fruits:\n",
    "    print(f)"
   ]
  }
